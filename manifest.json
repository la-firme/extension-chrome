{
    "manifest_version": 2,
    "name": "La Firme",
    "description": "Soyez notifiez quand La Firme est en live !",
    "version": "1.0.0",
    "background": {
        "scripts": ["script.js"]
    },
    "browser_action": {
        "default_icon": "icon/iconon48.png"
    },
    "icons": {
        "16": "icon/iconon16.png",
        "32": "icon/iconon32.png",
        "48": "icon/iconon48.png",
        "128": "icon/iconon128.png"
    },
    "permissions": [
        "notifications",
        "https://api.twitch.tv/"
    ]
}
